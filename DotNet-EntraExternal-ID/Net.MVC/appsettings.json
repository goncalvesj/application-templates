{
  "AzureAdB2C": {
    "Instance": "https://xxxx.b2clogin.com",
    "Domain": "xxxx.onmicrosoft.com",
    "TenantId": "[tenantid]",
    "ClientId": "[appregistrationid]",
    "ClientSecret": "",
    "SignUpSignInPolicyId": "B2C_1_SignUpSignInWeb",
    "CallbackPath": "/signin-oidc",
    "ResponseType": "code",
    "UsePkce": true,
    "Scope": [ "openid", "offline_access" ]
  },
  "WeatherApi": {
    "Scope": "https://xxxx.onmicrosoft.com/[appregistrationid]/weather.read",
    "BaseAddress": "https://localhost:7093"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
